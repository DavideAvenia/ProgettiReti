          fx:controller="View.VisualizzaRider">
   <children>
      <Pane prefHeight="600.0" prefWidth="227.0">
         <children>
            <ListView fx:id="listaRider" layoutX="14.0" layoutY="46.0" prefHeight="406.0" prefWidth="200.0" />
            <Button fx:id="buttoneProcediOrdine" layoutX="60.0" layoutY="548.0" mnemonicParsing="false" onAction="#procediOrdine" text="Procedi all'ordine" />
            <Label layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0" text="Visualizzazione dei rider" textAlignment="CENTER" />
            <Label layoutX="14.0" layoutY="466.0" text="Rider selezionato:" />
            <Label fx:id="labelRiderSelezionato" layoutX="100.0" layoutY="492.0" />
         </children>
      </Pane>
   </children>
</GridPane>
